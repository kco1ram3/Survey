<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SurveyModel" namespace="Survey">
  <class name="LikertItemResponse">
    <!-- Primary key-->
    <id name="ID" type="Int32" unsaved-value="0">
      <generator class="native" />
    </id>
    <property name="SequenceNo" />
    <many-to-one name="Parent" class="LikertResponse" column="ParentID" />
    <many-to-one name="LikertItem" class="LikertItem" column="LikertItemID" />
    <many-to-one name="Choice" class="Choice" column="ChoiceID" />
  </class>
</hibernate-mapping>